{
    "name": "alemohamad/ultralight-web-kit",
    "description": "Ultralight Web Kit is a base Flight project for small websites.",
    "keywords": ["flightphp", "flight", "mvc", "ultralight", "web", "kit", "website", "skeleton"],
    "homepage": "https://github.com/alemohamad/ultralight-web-kit",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Ale Mohamad",
            "email": "hello@alemohamad.com",
            "homepage": "https://alemohamad.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "mikecao/flight": "^1.3",
        "twig/twig": "^1.34",
        "vlucas/phpdotenv": "^2.4",
        "phpmailer/phpmailer": "^5.2",
        "vlucas/valitron": "^1.4",
        "joelvardy/flash": "dev-master",
        "illuminate/database": "^4.2",
        "robmorgan/phinx": "^0.8.1"
    },
    "prefer-stable" : true,
    "minimum-stability": "dev",
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "Controllers\\": "app/Controllers",
            "Models\\": "app/Models",
            "Config\\": "app/Config",
            "Auth\\": "app/Auth"
        }
    }
}
